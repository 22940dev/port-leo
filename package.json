{
  "name": "leo",
  "version": "3.0.5",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "test": "eslint .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "yarn test",
      "prettier --single-quote --no-semi --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "markdown-in-js/babel"
    ]
  },
  "license": "MIT",
  "repository": "leo/site",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "prettier",
      "prettier/react",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/display-name": 0,
      "react/prop-types": 0
    }
  },
  "dependencies": {
    "@babel/helper-create-regexp-features-plugin": "7.12.7",
    "is-absolute-url": "3.0.3",
    "markdown-in-js": "1.1.4",
    "moment": "2.29.1",
    "next": "10.0.3",
    "nprogress": "0.2.0",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-codepen": "0.1.0",
    "react-dom": "17.0.1",
    "react-syntax-highlighter": "15.3.1",
    "react-tweet-embed": "1.2.2",
    "to-id": "2.0.0"
  },
  "devDependencies": {
    "@types/node": "14.14.10",
    "@types/react": "17.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-react": "7.21.5",
    "husky": "4.3.4",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "4.1.2",
    "xo": "0.35.0"
  }
}

{
  "name": "leo",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "xo"
  },
  "xo": {
    "extends": "xo-react",
    "space": true,
    "semicolon": false,
    "env": [
      "browser"
    ],
    "rules": {
      "react/react-in-jsx-scope": 0,
      "react/jsx-pascal-case": 0,
      "object-curly-spacing": 0,
      "react/jsx-curly-spacing": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-unresolved": 0,
      "import/named": 0,
      "react/prop-types": 0,
      "react/forbid-component-props": 0
    }
  },
  "now": {
    "alias": "leo.im"
  },
  "dependencies": {
    "is-absolute-url": "2.1.0",
    "moment": "2.17.1",
    "next": "2.0.0-beta.24",
    "nprogress": "0.2.0",
    "react-codepen": "0.1.0",
    "react-syntax-highlighter": "4.0.1",
    "react-tweet-embed": "1.0.3",
    "to-id": "1.0.5"
  },
  "devDependencies": {
    "eslint-config-xo-react": "0.10.0",
    "eslint-plugin-react": "6.9.0",
    "xo": "0.17.1"
  }
}
